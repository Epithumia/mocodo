{
  "extension": ".md",
  "highlight": "markdown",
  "compose_primary_key": "<ins>{label}</ins>",
  "compose_alt_key": "{label}<sup>u{id_groups}</sup>",
  "compose_foreign_key": "_#{label}_",
  "compose_primary_foreign_key": "<ins>_#{label}_</ins>",
  "compose_relation": "**{this_relation_name}** ({columns})",
  "relation_separator": "<br>\n",
  "transform_relation": [
    {
      "order": 100,
      "search": "(<sup>u\\d+)(\\d)",
      "replace": "\\1 u\\2",
      "iterated": true
    }
  ],
  "transform_relational_schema": [
    {
      "order": 100,
      "search": "\n--><br>\n",
      "replace": "\n-->\n"
    }
  ]
}