Project basic {
  Note: 'Generated by Mocodo'
}

Table CLIENT {
  "Réf. client" VARCHAR(42) [pk]
  Nom VARCHAR(42)
  "Prénom" VARCHAR(42)
  Adresse VARCHAR(42)
}

Table COMMANDE {
  "Num commande" VARCHAR(42) [pk]
  Date VARCHAR(42)
  Montant VARCHAR(42)
}

Table INCLURE {
  "Num commande" VARCHAR(42)
  "Réf. produit" VARCHAR(42)
  "Quantité" VARCHAR(42)
  Indexes {
    ("Num commande", "Réf. produit") [pk]
  }
}

Table PASSER {
  "Réf. client" VARCHAR(42)
  "Num commande" VARCHAR(42)
  Indexes {
    ("Réf. client", "Num commande") [pk]
  }
}

Table PRODUIT {
  "Réf. produit" VARCHAR(42) [pk]
  "Libellé" VARCHAR(42)
  "Prix unitaire" VARCHAR(42)
}

Ref:INCLURE."Num commande" > COMMANDE."Num commande"
Ref:INCLURE."Réf. produit" > PRODUIT."Réf. produit"
Ref:PASSER."Réf. client" > CLIENT."Réf. client"
Ref:PASSER."Num commande" > COMMANDE."Num commande"
